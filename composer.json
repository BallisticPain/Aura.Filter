{
    "name": "aura/filter",
    "type": "library",
    "description": "Validate and sanitize objects and arrays.",
    "keywords": [
        "filter",
        "validate",
        "sanitize",
        "validator",
        "validation"
    ],
    "homepage": "http://auraphp.github.com/Aura.Filter",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Aura Contributors",
            "homepage": "https://github.com/auraphp/Aura.Filter/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-4": {
            "Aura\\Filter\\_Config\\": "config/",
            "Aura\\Filter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aura\\Filter\\": "tests"
        }
    },
    "extra": {
        "aura": {
            "type": "library",
            "config": {
                "common": "Aura\\Filter\\_Config\\Common"
            }
        },
        "branch-alias": {
            "dev-develop-2": "2.0.x-dev"
        }
    }
