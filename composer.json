{
    "name": "aura/filter",
    "type": "aura-package",
    "description": "The Aura Filter package provider validators and sanitizers for user data.",
    "keywords": [ "filter", "validate", "sanitize", "validator", "validation" ],
    "homepage": "http://auraphp.github.com/Aura.Filter",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Aura Contributors",
            "homepage": "https://github.com/auraphp/Aura.Filter/contributors"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "autoload": {
        "psr-4": {
            "Aura\\Filter\\_Config\\": "config/",
            "Aura\\Filter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aura\\Filter\\": "tests"
        }
    },
    "extra": {
        "aura": {
            "type": "library",
            "config": {
                "common": "Aura\\Filter\\_Config\\Common"                
            }
        }
    }
}
